{{#each model as |notification|}}
	{{#wds-banner-notification
		type=notification.type
		onClose=(action 'onClose' notification.id)
	}}
		{{#if notification.text}}
			{{notification.text}}
		{{else if notification.alreadySafeHtml}}
			{{{notification.alreadySafeHtml}}}
		{{/if}}
	{{/wds-banner-notification}}
{{/each}}
