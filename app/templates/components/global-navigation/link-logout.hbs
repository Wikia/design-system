{{! Logout is a POST, so we render a button inside a form here }}
<form method="POST" action={{model.href}}>
  <input
    type="hidden"
    name={{model.param-name}}
    value={{redirectUrl}}
  >
  {{! wds-sign-out__button will make this look like a link }}
  <button
    type="submit"
    class="wds-sign-out__button"
    data-tracking-label={{model.tracking-label}}
  >
    {{i18n model.title.key ns="design-system"}}
  </button>
</form>
