{
  "name": "@fandom/design-system",
  "version": "23.21.0",
  "description": "Design System developed for Fandom",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wikia/design-system.git"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "scripts": {
    "prepare": "yarn build",
    "test": "ember test",
    "test-existing": "ember test --path=\"test-dist\"",
    "test:jenkins": "yarn test",
    "ci": "yarn install && yarn build",
    "dev": "ember s",
    "release-major": "yarn version --major && yarn publish",
    "release-minor": "yarn version --minor && yarn publish",
    "release-patch": "yarn version --patch && yarn publish",
    "release-test": "yarn version --prerelease --preid test && yarn publish",
    "build": "yarn clean:build && yarn styles && yarn svg && cp -R style-guide/styles dist/scss",
    "build:docs": "yarn build && yarn clean:docs && ember b -prod && mv docs-dev/ docs && cp CNAME docs/",
    "build-test": "ember build --output-path=\"test-dist\" --environment=test",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:jenkins": "yarn lint:js",
    "clean:build": "rm -rf dist && mkdir dist",
    "clean:docs": "rm -rf docs-dev docs",
    "svg": "yarn svg:svgo-source && yarn svg:copy && yarn svg:sprite && yarn svg:svgo-dist",
    "svg:copy": "mkdir -p dist/svg && cp style-guide/assets/*.svg dist/svg/",
    "svg:sprite": "node ./scripts/create-svg-sprite.js",
    "svg:svgo-dist": "svgo --config .svgo-dist.yml --quiet --folder dist/svg",
    "svg:svgo-source": "svgo --precision 3 --config .svgo-source.yml --quiet --folder style-guide/assets",
    "styles": "sass --no-source-map style-guide/styles/index.scss dist/css/styles.css"
  },
  "devDependencies": {
    "@ember/optional-features": "0.7.0",
    "@wikia/ember-fandom": "github:Wikia/ember-fandom#a35255d484115a9e7bc114b926418afa7a9c5829",
    "broccoli-asset-rev": "3.0.0",
    "broccoli-funnel": "2.0.1",
    "broccoli-svgstore": "0.4.2",
    "ember-browserify": "1.2.2",
    "ember-cli": "3.6.0",
    "ember-cli-autoprefixer": "0.8.1",
    "ember-cli-code-coverage": "1.0.0-beta.7",
    "ember-cli-dependency-checker": "3.0.0",
    "ember-cli-deploy": "1.0.2",
    "ember-cli-deploy-build": "1.1.1",
    "ember-cli-deploy-sftp": "0.1.2",
    "ember-cli-deprecation-workflow": "1.0.1",
    "ember-cli-eslint": "5.0.0",
    "ember-cli-htmlbars": "3.0.1",
    "ember-cli-htmlbars-inline-precompile": "2.1.0",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-inline-content": "0.4.1",
    "ember-cli-json-module": "1.0.0",
    "ember-cli-numeral": "1.0.0",
    "ember-cli-page-object": "1.15.1",
    "ember-cli-sass": "10.0.0",
    "ember-cli-template-lint": "github:ember-template-lint/ember-cli-template-lint#v1.0.0-beta.2",
    "ember-cli-uglify": "2.1.0",
    "ember-code-snippet": "2.3.1",
    "ember-export-application-global": "2.0.0",
    "ember-fetch": "6.4.0",
    "ember-load-initializers": "2.0.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-one-way-controls": "3.1.0",
    "ember-qunit": "4.2.0",
    "ember-resolver": "5.0.1",
    "ember-router-scroll": "1.0.1",
    "ember-source": "3.6.1",
    "ember-source-channel-url": "1.1.0",
    "ember-truth-helpers": "2.1.0",
    "ember-uuid": "1.0.1",
    "eslint-plugin-ember": "6.1.0",
    "eslint-plugin-node": "8.0.0",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "highlight.js": "9.13.1",
    "i18next": "10.5.0",
    "loader.js": "4.7.0",
    "qunit-dom": "0.8.3",
    "sass": "1.30.0",
    "svgo": "1.1.1",
    "svgstore": "3.0.0-2"
  },
  "bugs": {
    "url": "https://github.com/Wikia/design-system/issues"
  },
  "homepage": "https://github.com/Wikia/design-system#readme",
  "dependencies": {
    "ember-autoresize": "1.3.2",
    "ember-cli-babel": "7.2.0",
    "ember-cli-js-cookie": "0.1.1"
  },
  "peerDependencies": {
    "ember-fetch": "*",
    "i18next": "10.5.0"
  }
}
